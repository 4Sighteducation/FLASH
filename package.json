{
  "name": "flash",
  "version": "1.0.4",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "echo 'Running with mock IAP - real purchases require custom build' && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "iap:mock": "node scripts/toggle-iap.js mock",
    "iap:real": "node scripts/toggle-iap.js real",
    "build:android": "npm run iap:real && eas build --platform android",
    "build:ios": "npm run iap:real && eas build --platform ios",
    "build:android:dev": "npm run iap:real && eas build --platform android --profile development",
    "build:ios:dev": "npm run iap:real && eas build --platform ios --profile development",
    "postinstall": "patch-package",
    "vercel-build": "npx expo export --platform web",
    "build:web": "npx expo export --platform web && cp -r api dist/api && cp vercel.json dist/vercel.json",
    "deploy:web": "npm run build:web && cd dist && vercel --prod"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.5.0",
    "expo": "^54.0.29",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-build-properties": "~1.0.10",
    "expo-constants": "~18.0.12",
    "expo-crypto": "~15.0.8",
    "expo-device": "~8.0.10",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.10",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-notifications": "~0.32.15",
    "expo-secure-store": "~15.0.8",
    "expo-speech": "~14.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "openai": "^5.3.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.1",
    "react-native-worklets-core": "^1.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.16",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
