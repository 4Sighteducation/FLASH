{
  "name": "flash",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "echo 'Running with mock IAP - real purchases require custom build' && expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "iap:mock": "node scripts/toggle-iap.js mock",
    "iap:real": "node scripts/toggle-iap.js real",
    "build:android": "npm run iap:real && eas build --platform android",
    "build:ios": "npm run iap:real && eas build --platform ios",
    "build:android:dev": "npm run iap:real && eas build --platform android --profile development",
    "build:ios:dev": "npm run iap:real && eas build --platform ios --profile development"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.5.0",
    "expo": "~53.0.0",
    "expo-audio": "^0.4.6",
    "expo-auth-session": "^6.2.0",
    "expo-av": "^15.1.6",
    "expo-constants": "~17.0.3",
    "expo-crypto": "^14.1.5",
    "expo-dev-client": "^5.2.0",
    "expo-device": "~7.0.2",
    "expo-file-system": "^18.1.10",
    "expo-image-manipulator": "^13.1.7",
    "expo-image-picker": "^16.1.4",
    "expo-in-app-purchases": "^14.5.0",
    "expo-linear-gradient": "^14.1.5",
    "expo-notifications": "~0.29.13",
    "expo-secure-store": "~14.2.3",
    "expo-speech": "^13.1.7",
    "expo-status-bar": "~2.2.3",
    "expo-web-browser": "^14.1.6",
    "openai": "^5.3.0",
    "react": "19.0.0",
    "react-native": "0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-url-polyfill": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-expo": "^8.0.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.0",
    "typescript": "^5.1.3"
  },
  "private": true
}
