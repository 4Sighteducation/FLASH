{
  "name": "flash",
  "version": "1.0.4",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dev": "echo 'Running with mock IAP - real purchases require custom build' && expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx",
    "iap:mock": "node scripts/toggle-iap.js mock",
    "iap:real": "node scripts/toggle-iap.js real",
    "build:android": "npm run iap:real && eas build --platform android",
    "build:ios": "npm run iap:real && eas build --platform ios",
    "build:android:dev": "npm run iap:real && eas build --platform android --profile development",
    "build:ios:dev": "npm run iap:real && eas build --platform ios --profile development",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "~1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@supabase/supabase-js": "^2.39.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "dotenv": "^16.5.0",
    "expo": "~51.0.14",
    "expo-audio": "^1.0.13",
    "expo-auth-session": "~5.5.0",
    "expo-constants": "~16.0.2",
    "expo-crypto": "~13.0.2",
    "expo-device": "~6.0.2",
    "expo-file-system": "~17.0.1",
    "expo-font": "~12.0.10",
    "expo-image-manipulator": "~12.0.5",
    "expo-image-picker": "~15.1.0",
    "expo-linear-gradient": "~13.0.2",
    "expo-notifications": "~0.28.9",
    "expo-secure-store": "~13.0.1",
    "expo-speech": "~12.0.2",
    "expo-status-bar": "~1.12.1",
    "expo-web-browser": "~13.0.3",
    "openai": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.5",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "~3.31.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.79",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-expo": "~7.1.2",
    "jest": "^29.2.1",
    "jest-expo": "~51.0.1",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "typescript": "~5.3.3"
  },
  "private": true
}
